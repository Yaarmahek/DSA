#include<iostream>
using namespace std;
int main()
{
	int key,n;
	cout<<"enter the size of array you want to enter:"<<endl;
	cin>>n;
	int arr[n];
	
	cout<<"enter the element:"<<endl;
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	
	cout<<"enter key to be search:";
	cin>>key;
	
	for(int i=0;i<n;i++)
	{
		if(arr[i]==key)
		{
			cout<< "element "<<key<<"found at index"<<i<<endl;
			return 0;
		}
	
	}
	cout<<"element"<<key<<" not found";
	return 0;
}
